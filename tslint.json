{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": true,
    "quotemark": [
      true,
      "single"
    ],
    "no-namespace": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "no-console": false
  },
  "include": [
    "src//*.ts",
    "src//*.tsc"
  ],
  "rulesDirectory": []
}
