{
  "manifest_version": 2,
  "name": "com.heiliuer.sdic",
  "version": "0.0.1",
  "background": {
    "scripts": [
      "js/jquery-2.0.0.min.js",
      "js/background.js"
    ]
  },
  "icons": {
    "16": "img/logo.png",
    "48": "img/logo.png",
    "128": "img/logo.png"
  },
  "page_action": {
    "default_icon": {
      "19": "img/logo.png",
      "38": "img/logo.png"
    },
    "default_title": "简词典",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "http://fanyi.youdao.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/jquery-2.0.0.min.js",
        "js/content_script_tmpl.js",
        "js/content_script.js"
      ],
      "run_at1": "document_start",
      "run_at": "document_start"
    }
  ]
}
